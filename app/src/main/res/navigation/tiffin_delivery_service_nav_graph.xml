<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/tiffin_delivery_service_nav_graph"
    app:startDestination="@id/tiffinServicesListFragment">

    <fragment
        android:id="@+id/tiffinServicesListFragment"
        android:name="com.example.tiffindeliveryapp.user.TiffinServicesListFragment"
        android:label="fragment_tiffin_services_list"
        tools:layout="@layout/fragment_tiffin_services_list" >
        <action
            android:id="@+id/action_tiffinServicesListFragment_to_serviceDetailsFragment"
            app:destination="@id/serviceDetailsFragment" />
        <action
            android:id="@+id/action_tiffinServicesListFragment_to_ordersListFragment2"
            app:destination="@id/ordersListFragment2" />
    </fragment>
    <fragment
        android:id="@+id/serviceDetailsFragment"
        android:name="com.example.tiffindeliveryapp.user.ServiceDetailsFragment"
        android:label="fragment_service_details"
        tools:layout="@layout/fragment_service_details" >
        <action
            android:id="@+id/action_serviceDetailsFragment_to_serviceSubscriptionFragment"
            app:destination="@id/serviceSubscriptionFragment" />
    </fragment>
    <fragment
        android:id="@+id/serviceSubscriptionFragment"
        android:name="com.example.tiffindeliveryapp.user.ServiceSubscriptionFragment"
        android:label="fragment_service_subscription"
        tools:layout="@layout/fragment_service_subscription" >
        <action
            android:id="@+id/action_serviceSubscriptionFragment_to_paymentFragment"
            app:destination="@id/paymentFragment" />
    </fragment>
    <fragment
        android:id="@+id/paymentFragment"
        android:name="com.example.tiffindeliveryapp.user.PaymentFragment"
        android:label="fragment_payment"
        tools:layout="@layout/fragment_payment" >
        <action
            android:id="@+id/action_paymentFragment_to_orderPlacedFragment"
            app:destination="@id/orderPlacedFragment" />
    </fragment>
    <fragment
        android:id="@+id/orderPlacedFragment"
        android:name="com.example.tiffindeliveryapp.user.OrderPlacedFragment"
        android:label="fragment_order_placed"
        tools:layout="@layout/fragment_order_placed" >
        <action
            android:id="@+id/action_orderPlacedFragment_to_tiffinServicesListFragment"
            app:destination="@id/tiffinServicesListFragment"
            app:popUpTo="@id/tiffinServicesListFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/ordersListFragment2"
        android:name="com.example.tiffindeliveryapp.OrdersListFragment"
        android:label="fragment_orders_list2"
        tools:layout="@layout/fragment_orders_list2" >
        <action
            android:id="@+id/action_ordersListFragment2_to_orderDetailsFragment"
            app:destination="@id/orderDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/orderDetailsFragment"
        android:name="com.example.tiffindeliveryapp.OrderDetailsFragment"
        android:label="fragment_order_details"
        tools:layout="@layout/fragment_order_details" />
</navigation>